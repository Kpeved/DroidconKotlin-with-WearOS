CREATE TABLE sponsor(
id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
name TEXT NOT NULL,
url TEXT,
icon TEXT,
groupName TEXT NOT NULL,
sponsorId TEXT,
description TEXT
);

insert:
INSERT INTO sponsor(name, url, icon, groupName, sponsorId, description)
VALUES (?,?,?,?,?,?)
;

selectAll:
SELECT * FROM sponsor;

sponsorById:
SELECT * FROM sponsor WHERE sponsorId = ?;

deleteAll:
DELETE FROM sponsor;